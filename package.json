{
  "name": "blog-graphql-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "migrate": "knex migrate:latest",
    "build": "babel src --out-dir dist --copy-files",
    "heroku-postbuild": "npm run clean && npm run migrate && npm run build",
    "start": "node dist/index.js",
    "dev": "nodemon --ext js,graphql --watch src --exec babel-node -r node_modules/dotenv/config src/index.js",
    "pretest": "cross-env NODE_ENV=test npm run migrate",
    "test": "jest -i",
    "test:watch": "npm run test -- --watch"
  },
  "author": "sarics",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sarics/blog-graphql-api.git"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "dependencies": {
    "apollo-server": "^2.2.2",
    "bcryptjs": "^2.4.3",
    "core-js": "^2.5.7",
    "cuid": "^2.1.4",
    "dotenv": "^6.1.0",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.15.2",
    "objection": "^1.4.0",
    "objection-db-errors": "^1.0.0",
    "pg": "^7.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "apollo-server-testing": "^2.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.6",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2"
  }
}
